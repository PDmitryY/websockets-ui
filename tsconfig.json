{
    "compilerOptions": {
      "target": "es2016",
      "module": "ES6",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "Node",
      "sourceMap": true,
      "allowJs": true,
    },
    "include": ["src/**/*"],
    "exclude": ["front/"]
}